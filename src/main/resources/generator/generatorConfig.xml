<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- targetRuntime="MyBatis3" -->
    <properties resource="application-alan.properties"/>
    <context id="Mysql" targetRuntime="MyBatis3" defaultModelType="flat">
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <!-- 扩展插件 -->
        <!--<plugin type="tk.mybatis.mapper.generator.MapperPlugin">-->
            <!--<property name="mappers" value="com.panda.util.MyMapper"/>-->
        <!--</plugin>-->
        <!-- 扩展插件 -->
        <!-- 注释 -->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <!-- 注释 -->
        <!-- 数据库链接 -->
        <jdbcConnection driverClass="${spring.datasource.driver-class-name}"
                        connectionURL="${spring.datasource.url}"
                        userId="${spring.datasource.username}"
                        password="${spring.datasource.password}">
        </jdbcConnection>
        <!-- 数据库链接 -->
        <!-- 存放位置 -->
        <javaModelGenerator targetPackage="com.panda.model.origin" targetProject="src/main/java"/>
        <sqlMapGenerator targetPackage="mapper.origin" targetProject="src/main/resources"/>
        <javaClientGenerator targetPackage="com.panda.mapper.origin" targetProject="src/main/java" type="XMLMAPPER"/>
        <!-- 存放位置 -->

        <!-- 数据表 -->


        <table tableName="e_machin_set" domainObjectName="MachinSet" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"/>
        <!--<table tableName="e_production_set" domainObjectName="ProductionSet" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"/>-->

        <!-- 数据表 -->
    </context>
